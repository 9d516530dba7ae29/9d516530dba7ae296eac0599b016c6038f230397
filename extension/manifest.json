{
  "manifest_version": 2,

  "name": "instiWebApp",
  "description": "IIT Patna Institute Web-App",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "iWebApp"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "notifications",
    "https://ajax.googleapis.com/",
    "http://www.iwebapp.ml/cAPI/*"
  ]
}
